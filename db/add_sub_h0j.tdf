--synthesis_resources = lut 74 
SUBDESIGN add_sub_h0j
( 
	add_sub	:	input;
	clken	:	input;
	clock	:	input;
	dataa[36..0]	:	input;
	datab[36..0]	:	input;
	result[36..0]	:	output;
) 
VARIABLE
	pipeline_dffe[36..0]	:	DFFE
		WITH (
			power_up ="low"
		);
	result_int[37..0]	:	WIRE;
BEGIN 
	result_int[] = (dataa[], !add_sub) + (!add_sub $ datab[], !add_sub);
	pipeline_dffe[].clk = clock;
	pipeline_dffe[].ena = clken;
	result[] = pipeline_dffe[36..0].q;
	pipeline_dffe[36..0].d = result_int[37..1];
END;
